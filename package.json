{
  "name": "deckardcain",
  "version": "0.1.6",
  "description": "Identifies (media) type of API description files",
  "main": "lib/deckardcain",
  "scripts": {
    "lint": "eslint src --ext .js,.es6 src",
    "precompile": "npm run lint",
    "compile": "babel src/ --out-dir lib/",
    "prepublish": "npm run compile",
    "preserver-test": "npm run compile",
    "prebrowser-test": "npm run compile",
    "server-test": "mocha --compilers es6:babel/register -R spec --recursive",
    "browser-test": "karma start",
    "test": "npm run server-test && npm run browser-test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/apiaryio/deckardcain"
  },
  "keywords": [
    "Apiary",
    "API Blueprint"
  ],
  "author": "Apiary Inc <support@apiary.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apiaryio/deckardcain/issues"
  },
  "homepage": "https://github.com/apiaryio/deckardcain",
  "devDependencies": {
    "babel": "^5.0.9",
    "babel-eslint": "^4.0.10",
    "babelify": "^6.2.0",
    "chai": "^3.2.0",
    "eslint": "^1.2.1",
    "eslint-config-airbnb": "0.0.8",
    "karma": "^0.13.3",
    "karma-browserify": "^4.2.1",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.4",
    "mocha": "^2.2.1"
  },
  "dependencies": {}
}
